{
  "name": "prosemirror-jdita",
  "version": "0.1.1",
  "description": "Use JDita in prosemirror",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "fix": "node scripts/fix",
    "postinstall": "yarn fix",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "build": "rimraf ./lib && tsc",
    "prepublishOnly": "yarn build",
    "test": "mocha",
    "test:watch": "mocha -w",
    "coverage": "rimraf coverage && nyc yarn mocha",
    "coveralls": "nyc --reporter=text-lcov report | coveralls",
    "generate-docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evolvedbinary/prosemirror-jdita.git"
  },
  "keywords": [
    "LwDITA",
    "DITA",
    "XML",
    "JSON"
  ],
  "author": "Evolved Binary",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/evolvedbinary/prosemirror-jdita/issues"
  },
  "homepage": "https://github.com/evolvedbinary/prosemirror-jdita#readme",
  "directories": {
    "src": "./src",
    "test": "./test",
    "scripts": "./scripts"
  },
  "files": [
    "lib",
    "scripts",
    "index.js"
  ],
  "dependencies": {
    "@types/chai-as-promised": "^7.1.3",
    "chai-as-promised": "^7.1.1",
    "jdita": "^0.2.4",
    "prosemirror-commands": "^1.5.2",
    "prosemirror-history": "^1.3.2",
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-menu": "^1.2.4",
    "prosemirror-model": "^1.19.4",
    "prosemirror-schema-list": "^1.3.0",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.33.5",
    "prosemirror-transform": "^1.8.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.6",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.3.10",
    "coveralls": "^3.1.1",
    "eslint": "^6.8.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "typedoc": "^0.25.7",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typescript": "4.6.3"
  }
}
